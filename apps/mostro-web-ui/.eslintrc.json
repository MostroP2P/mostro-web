{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["plugin:nuxt/recommended", "@mostro-web/eslint-config-custom"],
  "rules": {
    "@typescript-eslint/quotes": "off"
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "project": "./tsconfig.json",
    "sourceType": "module",
    "extraFileExtensions": [".vue"]
  }
}
